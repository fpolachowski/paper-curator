type Query {
    getDocuments : [Document]
    getDocumentById : Document
    findDocumentsByTitle : [Document]
}

type Document {
    title : String
    urls : [Link]
    shortDescription : String
    description : String
    date : String
    authors : [Author]
    documents : [String]
    categories : [String]
}

type Link {
    url : String
    contentType : String
}

type Author {
    name : String
}